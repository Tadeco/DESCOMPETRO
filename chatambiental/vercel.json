{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/home.html",
      "dest": "/index.html"
    },
    {
      "src": "/chat",
      "dest": "/index.html"
    },
    {
      "src": "/404",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {},
  "trailingSlash": false
}